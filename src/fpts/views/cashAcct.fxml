<Tab fx:id="acctTab" closable="false" text="Cash Account" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fpts.controllers.CashAccountController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
                <Pane fx:id="CashAccounts" maxWidth="250.0" minWidth="250.0" prefWidth="250.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <children>
                    <Text layoutX="50.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add a New Cash Account">
                        <font>
                            <Font name="Arial Bold" size="16.0" />
                        </font>
                    </Text>
                    <Label layoutX="50.0" layoutY="161.0" text="New Account Name:" />
                    <TextField fx:id="acctNameField" layoutX="30.0" layoutY="185.0" prefHeight="25.0" prefWidth="200.0" />
                    <Label layoutX="50.0" layoutY="220.0" text="Initial Deposit:" />
                    <TextField fx:id="acctBalanceField" layoutX="30.0" layoutY="242.0" prefHeight="2.0" prefWidth="200.0" />
                    <Text fill="#0b8613" layoutX="30.0" layoutY="259.0" strokeType="OUTSIDE" strokeWidth="0.0" text="\$">
                        <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                    </Text>
                    <Button fx:id="selectAcctBttn" layoutX="100.0" layoutY="317.0" mnemonicParsing="false" onAction="#selectAccount" text="Select" visible="false" />
                    <Button fx:id="createButton" layoutX="100.0" layoutY="337.0" mnemonicParsing="false" onAction="#createAccount" style="-fx-background-color: #5ac318; -fx-font-weight: bold;" text="Create" textFill="WHITE" />
                    <!--<Button fx:id="createButton" layoutX="424.0" layoutY="324.0" mnemonicParsing="false" onAction="#createAccount" style="-fx-background-color: #5ac318; -fx-font-weight: bold;" text="Create" textFill="WHITE" />-->
                    <Button fx:id="removeButton" disable="true" layoutX="100.0" layoutY="51.0" mnemonicParsing="false" onAction="#removeAcct" style="-fx-background-color: red; -fx-font-weight: bold;" text="Remove" />
                </children>
                </Pane>
              <TableView fx:id="cashAcctTable" prefHeight="394.0" prefWidth="304.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="250.0" AnchorPane.topAnchor="0.0">
                  <columns>
                      <TableColumn fx:id="acctName" prefWidth="109.0" text="Name" />
                      <TableColumn fx:id="acctBalance" prefWidth="94.0" text="Balance" />
                      <TableColumn fx:id="acctDate" prefWidth="99.0" text="Date" />
                  </columns>
              </TableView>
            </children></AnchorPane>
    </content>
</Tab>
